{
  "name": "@edx/frontend-component-footer-edx",
  "version": "1.0.0-semantically-released",
  "description": "Footer component for edX frontend applications",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "make build",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage"
  },
  "files": [
    "/dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -e $GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-component-footer-edx.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edx/frontend-component-footer-edx/issues"
  },
  "homepage": "https://github.com/edx/frontend-component-footer-edx#readme",
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-angular": "13.1.0",
    "@commitlint/prompt": "13.1.0",
    "@commitlint/prompt-cli": "13.1.0",
    "@edx/brand": "npm:@edx/brand-edx.org@2.0.3",
    "@edx/frontend-build": "5.6.14",
    "@edx/frontend-platform": "1.11.3",
    "@edx/paragon": "14.16.10",
    "codecov": "3.8.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "husky": "7.0.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-test-renderer": "16.14.0",
    "reactifex": "1.1.1",
    "redux": "4.1.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.25",
    "@fortawesome/free-brands-svg-icons": "5.11.2",
    "@fortawesome/free-regular-svg-icons": "5.11.2",
    "@fortawesome/free-solid-svg-icons": "5.11.2",
    "@fortawesome/react-fontawesome": "0.1.5"
  },
  "peerDependencies": {
    "@edx/frontend-platform": "^1.8.0",
    "prop-types": "^15.7.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
