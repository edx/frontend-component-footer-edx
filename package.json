{
  "name": "@edx/frontend-component-footer-edx",
  "version": "1.0.0-semantically-released",
  "description": "Footer component for edX frontend applications",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "make build",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage"
  },
  "files": [
    "/dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-component-footer-edx.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edx/frontend-component-footer-edx/issues"
  },
  "homepage": "https://github.com/edx/frontend-component-footer-edx#readme",
  "devDependencies": {
    "@edx/brand": "npm:@edx/brand-edx.org@2.1.2",
    "@edx/browserslist-config": "^1.1.1",
    "@edx/frontend-build": "12.8.27",
    "@edx/frontend-platform": "4.2.0",
    "@edx/paragon": "20.36.0",
    "@edx/reactifex": "2.1.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.7",
    "husky": "8.0.3",
    "prop-types": "15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.9",
    "react-router-dom": "5.3.4",
    "react-test-renderer": "16.14.0",
    "reactifex": "1.1.1",
    "redux": "4.2.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.4.0",
    "@fortawesome/free-brands-svg-icons": "6.4.0",
    "@fortawesome/free-regular-svg-icons": "6.4.0",
    "@fortawesome/free-solid-svg-icons": "6.4.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "js-cookie": "^3.0.1"
  },
  "peerDependencies": {
    "@edx/frontend-platform": "^4.0.0",
    "@edx/paragon": "^20.0.0",
    "prop-types": "^15.7.0",
    "react": "^16.9.0 || ^17.0.0",
    "react-dom": "^16.9.0 || ^17.0.0"
  }
}
